{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>renovate/renovate-config"
  ],
  "packageRules": [
    {
      "enabled": false,
      "matchPackageNames": [
        "*"
      ]
    },
    {
      "matchFileNames": [
        "@exlibris/exl-cloudapp-angular-lib",
        "@exlibris/exl-cloudapp-base",
        "@exlibris/exl-cloudapp-cli"
      ],
      "enabled": true
    }
  ]
}
